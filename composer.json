{
    "name": "sg/datatablesbundle",
    "type": "symfony-bundle",
    "description": "Symfony Datatable Bundle For Doctrine2 Entities with additional Elasticsearch functionality",
    "keywords": ["symfony", "datatable", "datatables", "data-table", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/eventit/DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        },
        {
            "name": "SgDatatablesBundle contributors",
            "homepage": "https://github.com/stwe/DatatablesBundle/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/eventit/DatatablesBundle/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eventit/FOSElasticaBundle.git"
        }
    ],
    "require": {
        "php": ">=7.2|>=8.0",
        "ext-json": ">=1.4",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/jsrouting-bundle": "^2.0|^3.0",
        "friendsofsymfony/elastica-bundle": "dev-php8",
        "symfony/config": "^4.4|^5.4|^6.0",
        "symfony/dependency-injection": "^4.4|^5.4|^6.0",
        "symfony/http-foundation": "^4.4|^5.4|^6.0",
        "symfony/http-kernel": "^4.4|^5.4|^6.0",
        "symfony/framework-bundle": "^4.4|^5.4|^6.0",
        "symfony/options-resolver": "^4.4|^5.4|^6.0",
        "symfony/property-access": "^4.4|^5.4|^6.0",
        "symfony/routing": "^4.4|^5.4|^6.0",
        "symfony/security-core": "^4.4|^5.4|^6.0",
        "symfony/translation": "^4.4|^5.4|^6.0",
        "twig/extra-bundle": "^3.5",
        "twig/twig": "^2.9|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "autoload": {
        "psr-4": { "Sg\\DatatablesBundle\\": "" }
    }
}
