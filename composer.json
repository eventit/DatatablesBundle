{
    "name": "sg/datatablesbundle",
    "type": "symfony-bundle",
    "description": "Symfony Datatable Bundle For Doctrine2 Entities with additional Elasticsearch functionality",
    "keywords": ["symfony3", "datatable", "datatables", "data-table", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/stwe/DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        },
        {
            "name": "SgDatatablesBundle contributors",
            "homepage": "https://github.com/stwe/DatatablesBundle/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/stwe/DatatablesBundle/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eventit/FOSElasticaBundle.git"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "ext-json": ">=1.4",
        "doctrine/orm": "^2.2",
        "friendsofsymfony/jsrouting-bundle": "^1.6|^2.0",
        "friendsofsymfony/elastica-bundle": "dev-php8",
        "twig/extensions": "^1.5",
        "twig/twig": "^1.28 || ^2.0",
        "symfony/translation": "^2.7|^3.0|^4.0",
        "symfony/options-resolver": "^3.0|^4.0",
        "symfony/property-access": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/security-bundle": "^2.1|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Sg\\DatatablesBundle\\": "" }
    }
}
